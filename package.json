{
  "name": "grassroots-map",
  "version": "1.0.0",
  "description": "A map for meditators who practice vipassana as taught by S.N. Goenka",
  "main": "index.js",
  "scripts": {
    "start": "npm run output-all-site-data && react-scripts start",
    "start-prod": "npm run build && http-server ./build/ -p 8081",
    "build": "npm run output-all-site-data && react-scripts build",
    "test-prod": "CYPRESS_baseUrl=http://localhost:8081 cypress run",
    "deploy": "npm run test-prod && surge --domain vipassana-grassroots-map.surge.sh ./build/",
    "output-student-count-data": "gsjson --worksheet student_counts 15bAkyPI-hJj8N0sQGU3f47CCvClewkx6YQAJ-ASC_M4 ./src/data/gitignored/oldStudentData.json",
    "output-group-sittings-data": "gsjson --worksheet group_sittings 15bAkyPI-hJj8N0sQGU3f47CCvClewkx6YQAJ-ASC_M4 ./src/data/gitignored/groupSittings.json",
    "recreate-gitignored-data-folder": "rm -rf ./src/data/gitignored && mkdir ./src/data/gitignored",
    "output-all-site-data": "npm run recreate-gitignored-data-folder && npm-run-all -p output-student-count-data output-group-sittings-data",
    "output-geojson-data": "node ./src/scripts/createRegions"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/leaflet": "^1.2.14",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-leaflet": "^1.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cypress": "^3.1.3",
    "eslint-plugin-cypress": "^2.2.0",
    "google-spreadsheet-to-json": "^1.0.0",
    "http-server": "^0.11.1",
    "npm-run-all": "^4.1.3",
    "surge": "^0.20.1",
    "typescript": "^3.2.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "leaflet": "^1.3.3",
    "postcss-modules": "^1.3.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-leaflet": "^2.1.4",
    "react-scripts": "^2.1.2",
    "wait-on": "^3.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
